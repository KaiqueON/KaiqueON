Programção - Olá Mundo :

Console.Write("olá ");
            Console.WriteLine("mundo !");
            Console.Write("Entre com seu nome: ");
            string nome  = Console.ReadLine();
            Console.Write("Bom dia, ");
            Console.Write(nome);
            Console.WriteLine("!");

Programação - Soma :

double num1, num2, soma;
            num1 = double.Parse(Console.ReadLine());
            num2 = double.Parse(Console.ReadLine());
            soma = num1 + num2;
            Console.WriteLine(soma);
            
           Programção - Valor da Area do Quadrado :
           
           double num1, mult;
            Console.Write(" digite o valor do lado do quadrado: ");
            num1 = double.Parse(Console.ReadLine());
            mult = num1 * num1;
            Console.WriteLine(mult);

Programação - Valor da area do Trapezio :

double basemaior, basemenor, altura, area;
      Console.WriteLine("digite base maior: ");
      basemaior = double.Parse (Console.ReadLine ());
     
      Console.WriteLine("digite base menor: ");
      basemenor = double.Parse (Console.ReadLine ());
     
      Console.WriteLine("digite altura: ");
      altura = double.Parse (Console.ReadLine ());
     
      area = (basemaior + basemenor)/2 * altura;
     
      Console.WriteLine ("A area: é " + area);
      
      Programação - Novo Salario :
      
      double salario, porcentagem, novo_salario;
      Console.WriteLine ("Digite o salario: ");
      salario = double.Parse (Console.ReadLine ());
     
      Console.WriteLine ("Digite o percentual: ");
      porcentagem = double.Parse (Console.ReadLine ());
     
      novo_salario = salario * porcentagem/100 + salario;
     
      Console.WriteLine ("Novo salario " + novo_salario );
      
      Programação - Calculo Tensão Eletrica :
      
       double  v, r ,i;
        Console.WriteLine ("Digite a corrente (I) :");
        i = double.Parse (Console.ReadLine());
       
        Console.WriteLine ("Digite a resistencia (R) :");
        r = double.Parse (Console.ReadLine());
       
        v = r * i;
       
        Console.WriteLine ("A tensão é : " +  v);
